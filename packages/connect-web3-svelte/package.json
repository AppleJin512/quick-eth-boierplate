{
  "name": "connect-web3-svelte",
  "version": "0.0.0",
  "type": "module",
  "main": "src/lib/index.ts",
  "svelte": "src/lib/index.ts",
  "scripts": {
    "dev": "svelte-kit package -w",
    "updateBuildPackageJSON": "node scripts/build.js",
    "build": "svelte-kit package && npm run updateBuildPackageJSON",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint ./src",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint-config-custom": "*",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.12",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4"
  }
}
